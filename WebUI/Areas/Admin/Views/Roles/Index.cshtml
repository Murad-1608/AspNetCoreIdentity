@model List<AdminModel.RoleViewModel>
@{
    ViewData["Title"] = "Index";
}




<div class="row">
    <div class="col-md-6 offset-md-2">
        <h2>Rollar</h2>
    </div>
    <div class="col-md-2">
        <a class="btn btn-primary" asp-controller="roles" asp-action="rolecreate">Rol əlavə et</a>
    </div>
</div>

<div class="row">
    <div class="col-md-8 offset-md-2">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Rol</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


